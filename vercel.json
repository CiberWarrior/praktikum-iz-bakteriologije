{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "astro",
  "rewrites": [
    {
      "source": "/Images/(.*)",
      "destination": "/Images/$1"
    }
  ],
  "routes": [
    {
      "src": "/poglavlje-5/vazni-testovi-u-determinaciji-koliformnih-bakterija",
      "dest": "/poglavlje-5/vazni-testovi-u-determinaciji-koliformnih-bakterija/index.html"
    },
    {
      "src": "/poglavlje-5/vazni-testovi-u-determinaciji-koliformnih-bakterija/",
      "dest": "/poglavlje-5/vazni-testovi-u-determinaciji-koliformnih-bakterija/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/Images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
